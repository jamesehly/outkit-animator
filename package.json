{
    "name": "outkit-animator",
    "version": "1.0.0",
    "description": "A simple animator class with timing functions",
    "main": "index.js",
    "scripts": {
        "test": "karma start",
        "prerelease": "npm run build",
        "release": "git add . && standard-version -a",
        "postrelease": "git push --follow-tags origin master && npm publish",
        "build": "rimraf ./dist && webpack && uglifyjs ./dist/outkit-animator.js -c -m -o ./dist/outkit-animator.min.js --source-map ./dist/outkit-animator.min.js.map --comments /Copyright/",
        "docs": "typedoc --out docs/api/ src/js/"
    },
    "author": "James Ehly",
    "license": "MIT",
    "devDependencies": {
        "@types/jasmine": "^2.5.54",
        "extract-text-webpack-plugin": "^3.0.0",
        "jasmine": "^2.8.0",
        "karma": "^1.7.1",
        "karma-chrome-launcher": "^2.2.0",
        "karma-jasmine": "^1.1.0",
        "karma-sourcemap-loader": "^0.3.7",
        "karma-typescript": "^3.0.5",
        "rimraf": "^2.6.1",
        "standard-version": "^4.2.0",
        "ts-loader": "^2.3.4",
        "typedoc": "^0.8.0",
        "typescript": "^2.5.2",
        "uglifyjs": "^2.4.11",
        "webpack": "^3.5.5"
    }
}